

<img class="info-window-image" src="https://loremflickr.com/120/120/nature?random=<%= rand(1..100) %>">
<h6><%= adventure.title %></h6>
<p><%= adventure.category.capitalize %></p>

<% adventure.avg_rating.round.times do %>
    <span class="fas fa-star"></span>
  <% end %>
  <% (5 - adventure.avg_rating.round).times do  %>
    <span class="far fa-star"></span>
  <% end %>



